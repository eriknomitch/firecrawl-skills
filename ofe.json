{
  "default": {
    "extensions": [
      ".py",
      ".json",
      ".md"
    ],
    "exclusions": [
      "node_modules/"
    ],
    "files": [
      "README.md"
    ],
    "first_file": "TODO.md"
  },
  "docs": {
    "extensions": [
      ".md"
    ],
    "first_file": "README.md"
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [
      ".git/config",
      ".gitignore"
    ]
  }
}
